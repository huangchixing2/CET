                    <!--android:layout_width="wrap_content"-->
                    <!--android:layout_height="match_parent"-->
                    <!--android:layout_marginLeft="10dp"-->
                    <!--android:src="@mipmap/arrow_down"/>-->
                    <!--</LinearLayout>-->
                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="10dp">

                    <RelativeLayout
                        android:id="@+id/equipment_working_item_rl"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:background="@color/cet_main_bg_color">

                        <TextView
                            android:id="@+id/equipment_working_title_address"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="18dp"
                            android:text="@string/cet_equipment_install_address"
                            android:textColor="@color/cet_main_nor_color"
                            android:textSize="15sp" />

                        <TextView
                            android:id="@+id/equipment_working_title_type"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="18dp"
                            android:layout_marginRight="5dp"
                            android:layout_toLeftOf="@+id/equipment_working_title_status"
                            android:layout_toRightOf="@+id/equipment_working_title_address"
                            android:gravity="center_horizontal"
                            android:text="@string/cet_equipment_title_type"
                            android:textColor="@color/cet_main_nor_color"
                            android:textSize="15sp" />

                        <TextView
                            android:id="@+id/equipment_working_title_status"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="10dp"
                            android:layout_toLeftOf="@id/equipment_working_title_communication"
                            android:text="@string/cet_equipment_title_statu"
                            android:textColor="@color/cet_main_nor_color"
                            android:textSize="15sp" />

                        <TextView
                            android:id="@+id/equipment_working_title_communication"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="5dp"
                            android:layout_toLeftOf="@+id/equipment_working_title_sim"
                            android:text="@string/cet_equipment_title_communication"
                            android:textColor="@color/cet_main_nor_color"
                            android:textSize="15sp" />

                        <TextView
                            android:id="@+id/equipment_working_title_sim"
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="5dp"
                            android:text="@string/cet_equipment_title_sim"
                            android:textColor="@color/cet_main_nor_color"
                            android:textSize="15sp" />

                    </RelativeLayout>

                    <ListView
                        android:id="@+id/cet_equipment_working_lv"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/equipment_working_item_rl"></ListView>

                </RelativeLayout>

            </LinearLayout>

            <include layout="@layout/layout_no_data" />

        </RelativeLayout>
    </LinearLayout>
